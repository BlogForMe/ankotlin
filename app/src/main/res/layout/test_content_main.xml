<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@android:color/white"
        android:padding="16dp">

    <com.android.util.view.BodyDataView
            android:id="@+id/rl_skeletalmuscle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_15"
            app:body_value_text_size="16sp"
            app:body_value_title="骨骼肌"
            app:body_value_txt="---" />

    <TextView
            android:id="@+id/dialog_show"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="Dialog_show"
            app:layout_constraintTop_toBottomOf="@+id/rl_skeletalmuscle" />


</androidx.constraintlayout.widget.ConstraintLayout>